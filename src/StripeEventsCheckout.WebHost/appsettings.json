{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithEnvironmentName"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning",
        "Duende" : "Warning",
        "Serilog.AspNetCore.RequestLoggingMiddleware" : "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Level:w}]: {Timestamp:dd-MM-yyyy:HH:mm:ss} {MachineName} {EnvironmentName} {SourceContext} {Message}{NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "ApplicationName": "Stripe Checkout Events Demo"
    }
  },
  "AllowedHosts": "*",
  "SeedProductData": true,
  "Stripe": {
    "PublishableKey": "",
    "SecretKey": "",
    "WebhookSecret" : ""
  },
  "ServiceBus" : {
    "Endpoint": "",
    "CheckoutEntityName": "stripe-checkout-events"
  },
  "OpenIdConnect": {
    "Authority" : "https://localhost:4244",
    "ClientId": "stripe.events.web",
    "ClientSecret" : "morpheus"
  }
}
